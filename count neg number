#include <iostream>
#include <vector>

using namespace std;

int countNegativeNumbers(const vector<int>& arr) {
    int count = 0;
    for (int num : arr) {
        if (num < 0) {
            count++;
        }
    }
    return count;
}

int main() {
    vector<int> array = {-1, 2, -3, 4, -5, 6};
    int result = countNegativeNumbers
